---
import BaseLayout from "../layouts/BaseLayout.astro";
import NavBar from "../components/NavBar.svelte";
import TBD from "../components/TBD.svelte";
import { tSeo } from "../lib/i18n/server";
import type { Locale } from "../lib/i18n/messages";

const lang = (Astro.currentLocale as Locale) || "en";
const seo = tSeo(lang);
---

<BaseLayout title={seo.projectsTitle} description={seo.projectsDescription} lang={lang}>
    <NavBar client:load />
    <TBD client:load pageKey="projects" />
</BaseLayout>
